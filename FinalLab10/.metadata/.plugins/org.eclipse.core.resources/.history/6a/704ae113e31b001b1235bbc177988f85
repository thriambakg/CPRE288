#include "ir.h"
#include "lcd.h"
#include "adc.h"
#include "ping (1).h"
#include "servo.h"
#include "timer.h"
#include "uart.h"
#include <inc/tm4c123gh6pm.h>
#include <math.h>
#include <stdlib.h>
#include "resetSimulation.h"
#include "s3-b_Scan.h"



void s3-b_init_Scan(void)
{
    timer_init();
    ping_init();
    adc_init();
    return;
}

void cyBOT_Scan(int angle)
{
    servo_move(angle);

    double irdist = adc_read();
    return;

}

