#include "adc.h"
#include "ping (1).h"
#include "servo.h"
#include "timer.h"
#include "s3-b_Scan.h"


void s3-b_init(void)
{
    timer_init();
    ping_init();
    adc_init();
    servo_init();
}

//moves by an angle

double s3-b_Scan(double angle)
{
    servo_move(angle);

    double irdist = adc_read();
    return irdist;

}

